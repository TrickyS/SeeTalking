<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="inu.capstone.duo.seetalking.NavigationActivity">

    <android.support.design.widget.BottomNavigationView
        android:id="@+id/talkactivity_bottomnavigationview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:visibility="visible"
        app:itemBackground="@android:color/black"
        app:itemIconTint="@android:color/white"
        app:itemTextColor="@android:color/white"
        app:menu="@menu/bottom_navigation" />

    <FrameLayout
        android:id="@+id/layout_frame_navi"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/talkactivity_bottomnavigationview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></FrameLayout>


</RelativeLayout>
